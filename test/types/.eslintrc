{
  "extends": ["airbnb-base"],
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "func-names": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0
  },
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": { "ecmaVersion": 6 }
}